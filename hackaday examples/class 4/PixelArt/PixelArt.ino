void setup() {
    Serial.begin(9600);
    delay( 3000 ); // power-up safety delay
    FastLED.addLeds<LED_TYPE, LED_PIN, COLOR_ORDER>(leds, NUM_LEDS).setCorrection( TypicalLEDStrip );
    FastLED.setBrightness(  BRIGHTNESS );
    fill_solid(leds, NUM_LEDS, CRGB::Black);
    FastLED.show();
}

void display_frame(int frame_number) {
  // the image converter has already done all the array conversion for us! all we need to do is
  // step through each location in the array and set the corresponding color.  
  
  for (int i = 0; i < NUM_LEDS; i++) { 
    leds[i] = frames[frame_number][i]; // this works because CRGB objects can be loaded using a hex color code! 

  /* // if you have trouble with the conversion, try printing your values :) or come to our office hours!  
   Serial.print(leds[i].r);
    Serial.print(" ");
    Serial.print(leds[i].g);
    Serial.print(" ");
    Serial.print(leds[i].b);
    Serial.println(""); */
  }
  FastLED.show();
}

void loop()
{
  // all we do is display each frame when the time comes, and then loop back around to the beginning when we hit NUM_FRAMES...
  if (millis() > lightingMillis) {
    display_frame(cur_frame);
    cur_frame++;
    if (cur_frame >= NUM_FRAMES) {
      cur_frame = 0;
    }
    lightingMillis = millis() + LIGHT_UPDATE_TIME; // the last thing inside your timer control block is always saving the target time for the next loop around! 
  }
}
